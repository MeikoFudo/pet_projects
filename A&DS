    {
      "cell_type": "code",
      "source": [
        "a = int(input())\n",
        "b = int(input())\n",
        "if a > b:\n",
        "  t = a\n",
        "  a = b\n",
        "  b = t\n",
        "while b%a!=0:\n",
        "  t = a\n",
        "  a = b%a\n",
        "  b = t\n",
        "print(\"Число \",a,\" является НОДом\")"
